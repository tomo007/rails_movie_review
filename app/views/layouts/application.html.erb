<!DOCTYPE html>
<html>
  <head>
    <title>Moviereview</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= render "layouts/header" %>
        <br/>
        <div class="container">
            <% if notice %>
              <%= render "layouts/alerts"%>
            <%end%>
            <h1> <%=  yield %></h1>
        </div>
  </body>
</html>
